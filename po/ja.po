#: src/prefs.c:122
#, c-format
msgid "gtkpod version %s usage:\n"
msgstr "gtkpod version %s 使い方:\n"

#: src/prefs.c:123
msgid "  -h, --help:   display this message\n"
msgstr "  -h, --help:   このヘルプを表示します\n"

#: src/prefs.c:124
msgid "  -m path:      define the mountpoint of your iPod\n"
msgstr "  -m path:      iPodのマウントポイントを指定します\n"

#: src/prefs.c:125
msgid "  --mountpoint: same as '-m'.\n"
msgstr "  --mountpoint: '-m'と同じ\n"

#: src/prefs.c:126
msgid "  -w:           write changed ID3 tags to file\n"
msgstr "  -w:           変更のあったID3タグをファイルに書き込む\n"

#: src/prefs.c:127
msgid "  --id3_write:   same as '-w'.\n"
msgstr "  --id3_write:   '-w'と同じ\n"

#: src/prefs.c:128
msgid "  -c:           check files automagically for duplicates\n"
msgstr "  -c:           ファイルの重複を自動的にチェックする\n"

#: src/prefs.c:129
msgid "  --md5:        same as '-c'.\n"
msgstr "  --md5:        '-c'と同じ\n"

#: src/prefs.c:130
msgid "  -a:           import database automatically after start.\n"
msgstr "  -a:           起動時にデータベースを自動的に読み込みます\n"

#: src/prefs.c:131
msgid "  --auto:       same as '-a'.\n"
msgstr "  --auto:       '-a'と同じ\n"

#: src/prefs.c:132
msgid ""
"  -o:           use offline mode. No changes are exported to the iPod,\n"
"                but to ~/.gtkpod/ instead. iPod is updated if 'Sync' is\n"
"                used with 'Offline' deactivated.\n"
msgstr ""
"  -o:           オフラインモードで起動します。すべての変更はiPod本体へは\n"
"                書き込まれません。代わりに ~/.gtkpod/ に書き込まれます。\n"
"                iPodへ書き込むには、「オフライン」を無効にした後、\n"
"                「更新」を実行してください。\n"

#: src/prefs.c:133
msgid "  --offline:    same as '-o'.\n"
msgstr "  --offline:    '-o'と同じ\n"

