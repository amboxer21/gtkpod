#: src/prefs.c:61
#, c-format
msgid "gtkpod version %s usage:\n"
msgstr "gtkpodバージョン%s 使い方:\n"

#: src/prefs.c:62
msgid "  -h, --help:   display this message\n"
msgstr "-h, --help   このヘルプを表示して終了します\n"

#: src/prefs.c:63
msgid "  -m path:      define the mountpoint of your iPod\n"
msgstr " -m path:      IPodのマウントポイントを定義します\n"

#: src/prefs.c:64
msgid "  --mountpoint: same as \"-m\".\n"
msgstr "--mountpoint:    \"-m\".と同じ\n"

#: src/prefs.c:65
msgid "  -w:           write changed ID3 tags to file\n"
msgstr "-w:         ファイルに変更されたID3タグを書込みます\n"

#: src/prefs.c:66
msgid "  --id3_write:   same as \"-w\".\n"
msgstr "--id3_write:    \"-w\".と同じ\n"

#: src/prefs.c:67
msgid "  -c:           check files automagically for duplicates\n"
msgstr " -c:     重複するファイルを自動的にチェックします\n"

#: src/prefs.c:68
msgid "  --md5:        same as \"-c\".\n"
msgstr "--md5:         \"-c\".と同じ\n"

#: src/prefs.c:69
msgid "  -a:           import database automatically after start.\n"
msgstr "-a:        起動した後、自動的にデータベースをインポートします\n"

#: src/prefs.c:70
msgid "  --auto:       same as \"-a\".\n"
msgstr "--auto:        \"-a\".と同じ\n"

#: src/prefs.c:71
msgid ""
"  -o:           use offline mode. No changes are exported to the iPod,\n"
"                but to ~/.gtkpod/ instead. iPod is updated if \"Export\" is\n"
"                used with \"Offline\" deactivated.\n"
msgstr ""
"-o:オフラインモードで使用してください。 No changes are exported to the "
"iPod,\n"
"but to ~/.gtkpod/ instead. iPod is updated if \"Export\" is\n"
"used with \"Offline\" deactivated.\n"

#: src/prefs.c:72
msgid "  --offline:    same as \"-o\".\n"
msgstr ""
"--offline:     \"-o\".\n"
"と同じ\n"

