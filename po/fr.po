msgid "  --id3_write:   same as \"-w\".\n"
msgstr "  --id3_write:  identique a \"-w\".\n"

#: src/prefs.c:124
msgid "  -c:           check files automagically for duplicates\n"
msgstr "  -c:           controle automatiquement les doublons\n"

