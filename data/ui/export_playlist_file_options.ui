                        <property name="can_focus">True</property>
                        <property name="tooltip-text" translatable="yes">If available, the local copy of the track is referenced in the playlist. Otherwise the file on the iPod is used.</property>
                        <property name="label" translatable="yes">_Prefer Local</property>
                        <property name="use_underline">True</property>
                        <property name="draw_indicator">True</property>
