                  </object>
                  <packing>
                    <property name="x_options"/>
                    <property name="y_options"/>
                  </packing>
                </child>
                <child>
                  <object class="GtkHBox" id="hbox65">
                    <property name="visible">True</property>
                    <child>
                      <object class="GtkLabel" id="label168">
                        <property name="visible">True</property>
